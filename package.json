		"reaviz": "^16.0.4",
		"stripe": "^17.7.0",
		"styled-components": "^6.1.15",
		"tailwind-datepicker-react": "^1.4.3",
		"uuid": "^11.0.5"
	},
	"devDependencies": {
		"@emotion/babel-plugin": "^11.13.5",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.49",
		"tailwindcss": "^3.4.15"
	}
